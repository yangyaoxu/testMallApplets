<!--components/w-swiper/w-swiper.wxml-->
<swiper class="swiper" circular autoplay 
      interval="3000" duration="300" indicator-dots indicator-active-color="#d4237a">
    <block wx:for="{{list}}" wx:key="index">
      <swiper-item>
      <image class="swiper-img" src="{{item.image}}" mode="widthFix"/>
      </swiper-item>
    </block>
</swiper>